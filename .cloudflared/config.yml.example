# Cloudflare Tunnel config for WorkTrack Pro
# 1. Copy to config.yml:  copy config.yml.example config.yml
# 2. Create a tunnel in Cloudflare Zero Trust dashboard and get tunnel ID + credentials path
# 3. Replace the placeholders below with your tunnel ID and credentials path
# 4. Run: cloudflared tunnel run

tunnel: YOUR_TUNNEL_ID
credentials-file: C:\Users\YOUR_USERNAME\.cloudflared\YOUR_TUNNEL_ID.json

ingress:
  - hostname: app.roughcutmfg.com
    service: http://localhost:4173
  - service: http_status:404
